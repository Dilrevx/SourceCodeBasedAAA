    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug feature extraction",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/feature_extraction.py",
            "console": "integratedTerminal",
            "args": [
                "market",
                "1",
                "3",
                "4"
            ]
        },
        {
            "name": "Debug acc",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/accuracy_analysis.py",
            "console": "integratedTerminal",
            "args": [
                "sdkid-creator-path",
                "customsmali_perm_lib"
            ]
        }
    ]
}